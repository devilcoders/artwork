<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= data.page.title || "Pixel Theme Example Using Artwork" %></title>
    
    <%= stylesheet_link_tag "http://basehold.it/24/cccccc", "framework" %>
    <%= javascript_include_tag  "artwork/artwork" %>
    <%= favicon_tag 'favicon.png' %>
  </head>
  
  <body class="<%= page_classes %>" data-grid-container>
    <header id="main-header" data-grid-row>
      <%= yield :header %>
    </header>
    
    <section id="main" data-grid-row>
      <div id="content" data-grid-column="12">
        <%= yield %>
      </div>
    </section>
      
      
    
  </body>
</html>